package "sxacl"
args "--unamed-opts --no-handle-version --no-handle-error --file-name=cmd_show --func-name=show_cmdline_parser --arg-struct-name=show_args_info"
usage "sxacl show [OPTIONS] sx://[profile@]cluster/volume"

section "Common options"

option  "config-dir"		c "Path to SX configuration directory"
        string typestr="PATH" optional hidden

option  "debug"		D "Enable debug messages" flag off
