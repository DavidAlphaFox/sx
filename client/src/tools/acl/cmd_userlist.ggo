package "sxacl"
args "--unamed-opts --no-handle-version --no-handle-error --file-name=cmd_userlist --func-name=userlist_cmdline_parser --arg-struct-name=userlist_args_info --strict-hidden"
usage "sxacl userlist [OPTIONS] sx://[profile@]cluster"

section "Common options"

option  "config-dir"		c "Path to SX configuration directory"
        string typestr="PATH" optional

option  "debug"		D "Enable debug messages" flag off
